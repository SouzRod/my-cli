{
    "name": "<%= completeName %>",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
      "check": "depcheck --ignores husky,mochawesome,babel-cli,eslint-config-airbnb,eslint-plugin-jsx-a11y,eslint-plugin-react<% if (type  === 'serverless'){ %>,serverless-iam-roles-per-function,serverless-offline,serverless-plugin-log-retention<% } %>",
      "start": "node index.js",
      "pretest": "npm run lint && npm run check",
      "test": "npm run test:coverage",
      "test:all": "npm run test:coverage && npm run test:integration",
      "test:unit": "mocha --recursive test/**/**/**/*.spec.js",
      "test:integration": "mocha --recursive test/**/**/**/*.spec.js",
      "test:coverage": "nyc --reporter=text --all mocha --recursive test/**/**/**/*.spec.js",
      "lint": "eslint *.js .",
      "lint:fix": "eslint --fix *.js .",
      "create-policy": "sls package && sls create-policy execute",
      "dev": "nodemon index.js"
    },
    "engines": {
      "node": "^16.14.0",
      "npm": "^8.3.1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "dotenv": "^8.0.0",<% if (type  === 'docker'){ %>
      "@hapi/hapi": "^20.2.1",
      "@hapi/inert": "^5.2.0",
      "@hapi/vision": "^5.5.2",
      "hapi-response-time": "^2.0.0",
      "hapi-swaggered": "^3.0.2",
      "hapi-swaggered-ui": "^3.0.1",
      "joi": "14.3.1"<% } %><% if (resource.includes('mongo')){ %>,
      "mongodb": "^3.1.1",
      "uuid": "^3.2.1"<% } %>
    },
    "devDependencies": {
      "chai": "^4.1.2",<% if (type  === 'serverless'){ %>
      "serverless": "2.10.0",
      "serverless-iam-roles-per-function": "^3.1.0",
      "serverless-offline": "^5.12.0",
      "serverless-plugin-log-retention": "^2.0.0",
      "serverless-plugin-log-subscription": "1.2.0",
      "serverless-pseudo-parameters": "^2.5.0",<% } %>
      "husky": "^4.3.8",
      "mocha": "^6.1.4",
      "nodemon": "^1.17.1",
      "nyc": "^14.1.1"
    },
    "nyc": {
      "check-coverage": true,
      "lines": 90,
      "statements": 90,
      "functions": 90,
      "branches": 90,
      "reporter": [
        "text",
        "text-summary",
        "html",
        "lcov"
      ],
      "reporting": {
        "print": "both"
      },
      "include": [
        "**/adapters/**/*.js"
      ],
      "exclude": [
        "test-report/**/*",
        "test/**/*",
        "commons/**/*",
        "coverage/**/*"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "npm test",
        "pre-push": "npm test"
      }
    }
  }
